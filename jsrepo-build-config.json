{
  "$schema": "https://unpkg.com/jsrepo@1.47.1/schemas/registry-config.json",
  "dirs": [
    "./src",
    "./src/blocks",
    "./src/collections",
    "./src/fields",
    "./src/utilities",
    "./src/providers",
    "./src/components"
  ],
  "doNotListBlocks": [],
  "doNotListCategories": [],
  "listBlocks": ["Testimonials"],
  "listCategories": [],
  "excludeDeps": ["next", "react"],
  "includeBlocks": [],
  "includeCategories": [],
  "excludeBlocks": [
    "Header",
    "Banner",
    "CallToAction",
    "MediaBlock",
    "RichText",
    "Code",
    "AdminBar",
    "BeforeDashboard"
  ],
  "excludeCategories": [],
  "preview": true,
  "rules": {
    "max-local-dependencies": ["warn", 20],
    "require-local-dependency-exists": "off",
    "no-circular-dependency": "off"
  }
}
